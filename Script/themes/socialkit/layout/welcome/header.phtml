    color: white;
    top: 50%;
    transform: translateY(-50%);
    background: #85b6e0;
    padding: 10px;
    border-radius: 4px;
}
.userProfile .msgWrap.left {
    left: 125%;
}
.userProfile .msgWrap.right {
    right: 125%;
}
.userProfile .msgWrap:before {
    content: "";
